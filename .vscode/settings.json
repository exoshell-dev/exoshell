{
  "[css][graphql][html][javascript][javascriptreact][json][jsonc][markdown][mdx][scss][typescript][typescriptreact][yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "aarch",
    "appindicator",
    "azuretools",
    "bierner",
    "bradlc",
    "clippy",
    "clsx",
    "contextmenu",
    "dbaeumer",
    "DEVCONTAINER",
    "eamodio",
    "esbenp",
    "euxo",
    "exoshell",
    "formulahendry",
    "foxundermoon",
    "getent",
    "gosu",
    "GROUPNAME",
    "hoovercj",
    "Huuums",
    "libayatana",
    "libgtk",
    "librsvg",
    "libwebkit",
    "lockb",
    "mantine",
    "meganrogge",
    "naumovs",
    "nodrag",
    "nonblock",
    "noprofile",
    "norc",
    "nprogress",
    "nsis",
    "nullglob",
    "pangea",
    "patchelf",
    "pipefail",
    "posthog",
    "quickbar",
    "reactflow",
    "RUSTUP",
    "serayuzgur",
    "shellcheck",
    "tailwindcss",
    "tamasfe",
    "tanstack",
    "tauri",
    "thiserror",
    "timonwong",
    "usermod",
    "vunguyentuan",
    "yatki"
  ],
  "cssVariables.lookupFiles": [
    "**/*.css",
    "**/*.scss",
    "**/*.sass",
    "**/*.less",
    "node_modules/@mantine/core/styles.css"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "editor.tabSize": 2,
  "files.associations": {
    "**/*.json": "jsonc",
    "**/package.json": "json",
    "*.css": "tailwindcss"
  },
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "json.schemas": [
    {
      "fileMatch": ["manifest.json"],
      "url": "https://json.schemastore.org/chrome-manifest"
    }
  ],
  "rust-analyzer.check.command": "clippy",
  "search.exclude": {
    "**/*.code-search": true,
    "**/_next": true,
    "**/bower_components": true,
    "**/node_modules": true
  },
  "turbo.path": "node_modules/turbo/bin/turbo",
  "turbo.useLocalTurbo": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "github.copilot.editor.enableAutoCompletions": true,
  "[dockerfile]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "cSpell.enableFiletypes": ["dockerfile"]
}
